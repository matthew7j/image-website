extends layout

block content
  div(class='images_container') 
    div(class='row flex')
      each image in image_list
        div(class='col s6 m4 l3 margin_bottom')
          div(class='card no_margin')
            div(class='card-image')
              a(href='/photography/image/' + image.id)
                img(src=image.signedUrl)